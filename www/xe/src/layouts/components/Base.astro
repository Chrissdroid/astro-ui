---
import Head from './Head.astro'

const {PageMeta,PageStyles}=Astro.props.meta
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <Head meta={PageMeta} styles={PageStyles} />
</head>
<body>
    <slot name="body"/>
</body>
</html>