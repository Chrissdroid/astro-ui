---
import {SEO} from 'astro-seo'
import {SocialImage} from 'astro-social-images'
import Stylesheet from 'astro-stylesheet'
import Font from 'astro-fonts'
const {meta,styles}=Astro.props
const stylesheets = [
    {
        href:Astro.resolve('../../styles/main.scss'),
        media:'screen'
    },...styles
]
---
   <SEO  
      title={meta?.page_title} 
      description={meta?.page_description}   
   />
   <!-- <SocialImage/>  -->
    <Font name="Roboto" />
    <Font name="Comforter" />
    <Stylesheet list={stylesheets}/>    