---
import Layout from '../../../layouts/landingLayout.astro' 
import getDocsRoutes from '../../../scripts/build/getDocRoutes'
import withMetaData from '../../../scripts/build/withMetaData'
let dataSet = []
await withMetaData(getDocsRoutes('guides')).then(res=>res.map(x=>dataSet.push({directory:x.props.directory, menu:x.props.menu})))
---
